<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xsd:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	
	<xsd:complexType name="NodeGraph">
		<xsd:sequence>
			<xsd:element name="Node" type="XSD:IDREF" />
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="Node">
		<xsd:attribute name="Id" type="xsd:ID" use="required"/>
		<xsd:sequence>
			<xsd:element name="Connected" type="xsd:IDREF" />
		</xsd:sequence>
	</xsd:complexType>
	
</xs:schema>
